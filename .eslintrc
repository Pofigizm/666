{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "quotes": [2, "single"],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "strict": [0],
    "no-new": [0],
    "space-unary-ops": [0],
    "no-unused-expressions": [0],
    "comma-dangle": [2, "never"],
    "no-var": [0],
    "no-console": [0]
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "window": true,
    "document": true,
    "google": true,
    "_": true,
    "Modernizr": true,
    "Image": true,
    "navigator": true
  }

}
